<div class="timeline">
    <nav class="js-timeline-navigation">
        <ul>
        {% for segment_id, segment in segments %}
            <li data-step="{{ segment_id }}"><a href="#notification-entity-{{ segment_id }}" class="js-timeline-link">{{ segment }}</a></li>
        {% endfor %}
        </ul>
    </nav>
    <div class="notifications">
        <a id="notification-entity-current"></a>
        <div id="notification-list">
            {{ notifications }}
        </div>
        <a id="notification-entity-start"></a>
    </div>
</div>