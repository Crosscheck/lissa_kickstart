{#
/**
 * @file
 * Default theme implementation to display an mpeg dash field.
 *
 * Available variables:
 * - video: A collection of image data.
 * - id: the id of the field.
 */
#}
<div class="dash-video-player js-dash-video-player" data-url="{{ url }}" data-id="{{ id }}">
    <video id="video-{{ id }}" controls="true"></video>
</div>